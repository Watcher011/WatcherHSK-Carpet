<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--======================================WallLights======================================-->

<ThingDef ParentName="SK_LampBase">
		<defName>HeadBarghestWall</defName>
		<label>Trophy Head of Barghest</label>
		<description>Trophy head of a defeated monster</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Furniture/Lights/HeadBarghest</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1.0,2.0)</drawSize>
			<drawRotated>true</drawRotated>
			<allowFlip>true</allowFlip>
		</graphicData>
		<uiIconPath>Things/Furniture/Lights/HeadBarghest_ico</uiIconPath>
		<altitudeLayer>PawnUnused</altitudeLayer>
		<fillPercent>0.15</fillPercent>
		<tickerType>Normal</tickerType>
		<drawerType>RealtimeOnly</drawerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<statBases>
			<WorkToBuild>80</WorkToBuild>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>0</Flammability>
			<Bulk>4</Bulk>
			<Mass>2</Mass>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">3</StyleDominance>
			<Beauty>8</Beauty>
		</statBases>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
		<size>(1,1)</size>
		<stuffCategories>
			<li>Woody</li>
			<li>Metallic</li>
			<li>Precious</li>
			<li>Ceramic</li>
		</stuffCategories>
		<costStuffCount>35</costStuffCount>
		<costList>
			<BargestHead>1</BargestHead>
		</costList>
		<surfaceType>Item</surfaceType>
		<placeWorkers>
			<li>SK.PlaceWorker_NotOnWall</li>
			<li>SK.PlaceWorker_WallAttachment</li>
		</placeWorkers>
		<drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
		<building>
			<uninstallWork>150</uninstallWork>
			<isEdifice>false</isEdifice>
			<canBuildNonEdificesUnder>true</canBuildNonEdificesUnder>
		</building>
		<clearBuildingArea>false</clearBuildingArea>
		<comps>
			<li Class="SK.CompProperties_RestrictedPlacement">
				<compClass>SK.CompRestrictedPlacement</compClass>
			</li>
		</comps>
		<constructionSkillPrerequisite>2</constructionSkillPrerequisite>
	</ThingDef>
	
